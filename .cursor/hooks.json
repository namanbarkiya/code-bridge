{
  "version": 1,
  "hooks": {
    "beforeShellExecution": [
      {
        "command": "node .cursor/hooks/notify-telegram.mjs",
        "timeout": 10
      }
    ],
    "afterAgentResponse": [
      {
        "command": "node .cursor/hooks/notify-telegram.mjs"
      }
    ],
    "stop": [
      {
        "command": "node .cursor/hooks/notify-telegram.mjs"
      }
    ]
  }
}
